<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="feed">
<insert id="new_feed" parameterType="com.project.sns.dto.FeedDTO" useGeneratedKeys="true" keyProperty="feed_id">
	<![CDATA[
		insert into feed(feed_title, feed_content, feed_user_id) 
		values 
		(#{feed_title}, #{feed_content}, #{feed_user_id});
	]]>
</insert>

<select id="get_feed_max" resultType="string">
	<![CDATA[
		select max(feed_id) from feed
	]]>
</select>
</mapper>