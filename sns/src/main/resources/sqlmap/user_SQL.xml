<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user">

<insert id="user_insert" parameterType="hashMap" useGeneratedKeys="true" keyProperty="user_num">
	<![CDATA[
		insert into user(user_id, user_pw) values (#{user_id}, #{user_pw});
	]]>	
</insert>
</mapper>
